{
    "env": {
        "browser": true,
        "mocha": true,
        "jest": true
    },
    "extends": "fortech",
    "rules": {
        "complexity": [ 1, 3 ],
        "comma-dangle": ["error", {
            "arrays": "never",
            "objects": "never",
            "imports": "never",
            "exports": "never",
            "functions": "ignore"
        }]
    }
}
